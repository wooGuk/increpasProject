<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 
	/*
	제 목 : bat.xml
	역 할 : Batting DB의 mapper.
	로 그 : 1.프로그램 최초 생성,당일 경기데이터 쿼리문 작성 (오우석 2016/06/21)
	*/
 -->
<mapper namespace="batting">
	<insert id="add" parameterType="java.util.Map">
		insert into batting(BAT_CODE,ID,MATCH_CODE,RE_RESULT,BAT_COST)
		values(bat_seq.nextval,#{ID},#{MATCH_CODE},#{RE_RESULT},#{BAT_COST})
	</insert>
</mapper>