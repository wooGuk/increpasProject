<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="test">

 	<!-- 로그인 -->
	<select id="login" parameterType="mybatis.vo.TestVO" resultType="mybatis.vo.TestVO">
		select * from test where ID = #{id} and pwd = #{pwd}
	</select>
	
	<select id="login2" parameterType="mybatis.vo.MemberVO" resultType="mybatis.vo.MemberVO">
		select * from member where id = #{id} and password = #{password}
	</select>
	
	<!-- 멤버추가 -->
	<insert id="add" parameterType="mybatis.vo.TestVO">
		insert into test(id,pwd,name,phone,address)
		values(#{id},#{pwd},#{name},#{phone},#{address})
	</insert>
	

	
</mapper>




